        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/continued-llc/api-guard",
    "authors": [
        {
            "name": "Elanor Riley",
            "email": "elanor.riley@continued.com"
        },
        {
            "name": "Chris Bautista",
            "email": "chrisbjr@gmail.com"
        },
        {
